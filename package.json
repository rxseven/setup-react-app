{
  "name": "setup-react-app",
  "description": "React app made easy.",
  "version": "0.1.0",
  "homepage": "https://github.com/rxseven/setup-react-app",
  "license": "MIT",
  "author": {
    "name": "Theerawat Pongpawat",
    "email": "rxseven.com@gmail.com",
    "url": "http://www.rxseven.com"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/rxseven/setup-react-app.git"
  },
  "private": true,
  "main": "index.jsx",
  "scripts": {
    "build": "node scripts/build.js",
    "format": "prettier-eslint --write src/**/*.{js,json,jsx,scss}",
    "lint:script": "eslint src/**/*.{js,jsx}",
    "lint:script:fix": "eslint --fix src/**/*.{js,jsx}",
    "lint:stylesheet": "stylelint \"src/**/*.scss\"",
    "precommit": "lint-staged",
    "start": "node scripts/start.js",
    "start:https": "HTTPS=true yarn start",
    "test": "node scripts/test.js --env=jsdom --config jest.config.json",
    "test:coverage": "npm test -- --coverage --no-cache",
    "test:debug": "react-scripts --inspect-brk test --runInBand --env=jsdom",
    "test:staged": "cross-env CI=true node scripts/test.js --env=jsdom --config jest.config.json --findRelatedTests",
    "type": "flow",
    "type:check": "flow check",
    "type:check:focus": "flow focus-check",
    "type:install": "flow-typed install"
  },
  "dependencies": {
    "chalk": "1.1.3",
    "classnames": "^2.2.6",
    "dotenv": "4.0.0",
    "dotenv-expand": "4.2.0",
    "fs-extra": "3.0.1",
    "lodash": "^4.17.10",
    "object-assign": "4.1.1",
    "promise": "8.0.1",
    "raf": "3.4.0",
    "ramda": "^0.25.0",
    "react": "^16.4.1",
    "react-dom": "^16.4.1",
    "react-hot-loader": "^4.3.3",
    "react-redux": "^5.0.7",
    "redux": "^4.0.0",
    "resolve": "1.6.0",
    "whatwg-fetch": "2.0.3"
  },
  "devDependencies": {
    "autoprefixer": "7.1.6",
    "babel-core": "6.26.0",
    "babel-eslint": "8.2.3",
    "babel-jest": "20.0.3",
    "babel-loader": "7.1.2",
    "babel-plugin-jsx-control-statements": "^3.2.8",
    "babel-plugin-react-css-modules": "^3.4.2",
    "babel-preset-react-app": "^3.1.1",
    "babel-runtime": "6.26.0",
    "case-sensitive-paths-webpack-plugin": "2.1.1",
    "cross-env": "^5.2.0",
    "css-loader": "0.28.7",
    "enzyme": "^3.3.0",
    "enzyme-adapter-react-16": "^1.1.1",
    "eslint": "4.10.0",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-config-react-app": "^2.1.0",
    "eslint-loader": "1.9.0",
    "eslint-plugin-flowtype": "2.39.1",
    "eslint-plugin-import": "2.8.0",
    "eslint-plugin-jest": "^21.17.0",
    "eslint-plugin-jsx-a11y": "^6.0.3",
    "eslint-plugin-jsx-control-statements": "^2.2.1",
    "eslint-plugin-react": "7.4.0",
    "extract-text-webpack-plugin": "3.0.2",
    "file-loader": "1.1.5",
    "flow-bin": "^0.74.0",
    "flow-typed": "^2.4.0",
    "html-webpack-plugin": "2.29.0",
    "husky": "^0.14.3",
    "jest": "20.0.4",
    "jest-enzyme": "^6.0.2",
    "lint-staged": "^7.2.0",
    "node-sass": "^4.9.0",
    "postcss-flexbugs-fixes": "3.2.0",
    "postcss-loader": "2.0.8",
    "postcss-scss": "^1.0.6",
    "prettier": "1.13.5",
    "prettier-eslint": "^8.8.1",
    "prettier-eslint-cli": "^4.7.1",
    "react-dev-utils": "^5.0.1",
    "react-test-renderer": "^16.4.1",
    "redux-mock-store": "^1.5.3",
    "sass-loader": "^7.0.3",
    "style-loader": "0.19.0",
    "stylelint": "^9.3.0",
    "stylelint-config-css-modules": "^1.2.0",
    "stylelint-config-recommended-scss": "^3.2.0",
    "stylelint-scss": "^3.1.3",
    "sw-precache-webpack-plugin": "0.11.4",
    "url-loader": "0.6.2",
    "webpack": "3.8.1",
    "webpack-dev-server": "2.9.4",
    "webpack-manifest-plugin": "1.3.2"
  }
}
